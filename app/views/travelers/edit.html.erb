<% if flash[:errors] %>
<% flash[:errors].each do |e| %>
<%= e %>
<% end %>
<% end %>


<%= form_for @traveler do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br>
    <%= f.label :Date_Of_Birth %>
    <%= f.date_field :dob %><br>

<%= f.label :password %>
    <%= f.password_field :password %><br>
<%= f.label :about_me %>
    <%= f.text_area :about_me %><br>
     <%= f.submit "Save Changes" %>
    <% end%>